<h3> <a class="btn btn-secondary" href="/student">Create New </a>student list</h3>
<table class="table table-striped">
    <thead>
        <tr>
        <th>Name</th>
        <th>Date_of_birth</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Address1</th>
        <th>Email_id</th>
        <th>Phone_No</th>
        <th>Alternate_Phone_No</th>
        <th>Subject_Choosen</th>
        <th>Class</th>
        <th>School_Name_College_Name</th>
        <th>Fathers_Name</th>
        <th>Mothers_Name</th>
        <th>Father_Ocupation</th>
        <th>Mothers_Ocupation</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.Name}}</td>
            <td>{{this.Date_of_birth}}</td>
            <td>{{this.Gender}}</td>
            <td>{{this.Address}}</td>
            <td>{{this.Address1}}</td>
            <td>{{this.Email_id}}</td>
            <td>{{this.Phone_No}}</td>
            <td>{{this.Alternate_Phone_No}}</td>
            <td>{{this.Subject_Choosen}}</td>
            <td>{{this.Class}}</td>
            <td>{{this.Semester}}</td>
            <td>{{this.SchoolName_CollegeName}}</td>
            <td>{{this.Fathers_Name}}</td>
            <td>{{this.Mothers_Name}}</td>
            <td>{{this.Fathers_Occupation}}</td>
            <td>{{this.Mothers_Occupation}}</td>
            <td>
                <a class="btn btn-primary btn-sm" href="/student/{{this.id}}">Edit</a>
                <a class="btn btn-primary btn-sm" href="/student/delete/{{this.id}}"
                onclick="return confirm('Are you sure you want to delete this record?')">delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>